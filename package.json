{
  "name": "imt-front-end-tools",
  "version": "1.0.0",
  "description": "Webpack 4 stater kit with SCSS, PostCSS, Babel & ESLint",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix 'src/**/*.{css,scss}'",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "npm run lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "npm run lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:all:markdownlint": "npm run lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test:e2e": "cypress open",
    "test:unit": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertguss/Webpack-Starter-Kit"
  },
  "keywords": [],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "airbnb-browser-shims": "^3.2.1",
    "autoprefixer": "^9.5.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-clean": "^1.1.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "terser": "^3.17.0",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "webpack": "^4.29.6",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-merge": "^4.2.1",
    "webpack-notifier": "^1.7.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-airbnb": "^3.2.0",
    "browser-sync": "^2.26.3",
    "css-loader": "^2.1.1",
    "cypress": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "0.14.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "resolve-url-loader": "^3.0.1",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "5.0.0",
    "stylelint-config-recess-order": "2.0.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "3.5.4",
    "typescript": "^3.3.4000",
    "vue-jest": "^3.0.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^0.14.10",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": [
    "defaults"
  ]
}
