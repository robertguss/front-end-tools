{
  "name": "imt-front-end-tools",
  "version": "1.0.0",
  "description": "Webpack 4 stater kit with TypeScript, PostCSS, Babel & ESLint",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "check-types": "tsc",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix 'src/**/*.{css,scss}'",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "npm run lint:eslint --ext .js",
    "lint:all:stylelint": "npm run lint:stylelint \"src/**/*.scss\"",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{js,json,css,scss,html}\"",
    "lint": "run-s lint:all:*",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "test:e2e": "testcafe 'chrome -incognito' tests/e2e/specs/",
    "test:unit": "jest"
  },
  "resolutions": {
    "webpack-dev-middleware": "3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "airbnb-browser-shims": "^3.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^7.0.26",
    "postcss-clean": "^1.1.0",
    "postcss-import": "^12.0.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "tailwindcss": "^1.1.4",
    "terser": "^4.6.3",
    "terser-webpack-plugin": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.8.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-airbnb": "^4.4.0",
    "browser-sync": "^2.26.7",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.0-beta.15",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "resolve-url-loader": "^3.1.1",
    "rucksack-css": "^1.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "stylelint": "13.0.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recess-order": "2.0.3",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "3.13.0",
    "testcafe": "^1.7.1",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "vuepress": "^1.2.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1"
  },
  "browserslist": [
    "defaults"
  ]
}
